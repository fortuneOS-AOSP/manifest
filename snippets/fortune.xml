<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- FortuneOS -->
  <remote  name="fortune"
           fetch="https://github.com/fortuneOS-AOSP"
           revision="usagi" />

  <remote  name="fortune-devices"
           fetch="https://github.com/Fortune-OS-Devices"
           revision="usagi" />

  <remote  name="fortune-gitea"
           fetch="https://gitea.com/FortuneOS"
           revision="usagi" />

  <!-- Roootdir -->
  <project path="art" name="art" groups="pdk" remote="fortune" />
  <project path="bionic" name="bionic" groups="pdk" remote="fortune" />
  <project path="fortune-keepers" name="fortune-keepers" groups="pdk" remote="fortune-devices" />
  <project path="manifest" name="manifest" remote="fortune" />

  <!-- Build -->
  <project path="build/make" name="build" groups="pdk" remote="fortune">
    <linkfile dest="build/CleanSpec.mk" src="CleanSpec.mk" />
    <linkfile dest="build/buildspec.mk.default" src="buildspec.mk.default" />
    <linkfile dest="build/core" src="core" />
    <linkfile dest="build/envsetup.sh" src="envsetup.sh" />
    <linkfile dest="build/target" src="target" />
    <linkfile dest="build/tools" src="tools" />
  </project>
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="fortune">
    <linkfile dest="Android.bp" src="root.bp" />
    <linkfile dest="bootstrap.bash" src="bootstrap.bash" />
  </project>

  <!-- Bootable -->
  <project path="bootable/recovery" name="bootable_recovery" groups="pdk" remote="fortune" />

  <!-- Device -->
  <project path="device/fortune/sepolicy" name="device_fortune_sepolicy" remote="fortune" />
  <project path="device/qcom/sepolicy-legacy-um" name="device_qcom_sepolicy" groups="qcom,sdm660" remote="fortune" revision="usagi-legacy-um"  />
  <project path="device/qcom/sepolicy_vndr/sm6225" name="device_qcom_sepolicy_vndr" revision="usagi-caf-sm6225" remote="fortune" />

  <!-- External -->
  <project path="external/tinycompress" name="external_tinycompress" groups="pdk" remote="fortune" />

  <!-- Frameworks -->
  <project path="frameworks/av" name="frameworks_av" remote="fortune" />
  <project path="frameworks/base" name="frameworks_base" remote="fortune" />
  <project path="frameworks/libs/systemui" name="frameworks_libs_systemui" remote="fortune" />
  <project path="frameworks/native" name="frameworks_native" remote="fortune" />

  <!-- Hardware -->
  <project path="hardware/interfaces" name="hardware_interfaces" remote="fortune" />
  <project path="hardware/libhardware" name="hardware_libhardware" remote="fortune" />
  <project path="hardware/qcom-caf/common" name="hardware_qcom-caf_common" groups="qcom" remote="fortune" >
    <!-- Add guard for AOSP hardware/qcom dir -->
    <linkfile src="os_pickup_aosp.mk" dest="hardware/qcom/Android.mk" />
    <!-- Add guards for CLO repositories -->
    <linkfile src="os_pickup_sepolicy_vndr.mk" dest="device/qcom/sepolicy_vndr/SEPolicy.mk" />
    <linkfile src="os_pickup_qssi.bp" dest="hardware/qcom-caf/msm8953/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/msm8953/Android.mk" />
    <linkfile src="os_pickup.bp" dest="hardware/qcom-caf/msm8996/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/msm8996/Android.mk" />
    <linkfile src="os_pickup.bp" dest="hardware/qcom-caf/msm8998/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/msm8998/Android.mk" />
    <linkfile src="os_pickup_qssi.bp" dest="hardware/qcom-caf/sdm660/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sdm660/Android.mk" />
    <linkfile src="os_pickup_qssi.bp" dest="hardware/qcom-caf/sdm845/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sdm845/Android.mk" />
    <linkfile src="os_pickup_qssi.bp" dest="hardware/qcom-caf/sm8150/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sm8150/Android.mk" />
    <linkfile src="os_pickup_qssi.bp" dest="hardware/qcom-caf/sm8250/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sm8250/Android.mk" />
    <linkfile src="os_pickup_qssi.bp" dest="hardware/qcom-caf/sm8350/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sm8350/Android.mk" />
    <linkfile src="os_pickup.bp" dest="kernel/google/gs101/private/google-modules/Android.bp" />
    <linkfile src="os_pickup_audio-ar.mk" dest="hardware/qcom-caf/sm6225/audio/Android.mk" />
    <linkfile src="os_pickup_qssi.bp" dest="hardware/qcom-caf/sm6225/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sm6225/Android.mk" />
    <linkfile src="os_pickup_audio-ar.mk" dest="hardware/qcom-caf/sm8450/audio/Android.mk" />
    <linkfile src="os_pickup_qssi.bp" dest="hardware/qcom-caf/sm8450/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sm8450/Android.mk" />
    <linkfile src="os_pickup_audio-ar.mk" dest="hardware/qcom-caf/sm8550/audio/Android.mk" />
    <linkfile src="os_pickup_qssi.bp" dest="hardware/qcom-caf/sm8550/Android.bp" />
    <linkfile src="os_pickup.mk" dest="hardware/qcom-caf/sm8550/Android.mk" />
    <!-- add guards for Pixel kernel modules -->
    <linkfile src="os_pickup.bp" dest="kernel/google/gs201/private/google-modules/Android.bp" />
  </project>
  <project path="hardware/qcom-caf/sm6225/audio/agm" name="vendor_qcom_opensource_agm" remote="fortune" revision="usagi-caf-sm6225" />
  <project path="hardware/qcom-caf/sm6225/audio/pal" name="vendor_qcom_opensource_arpal-lx" remote="fortune" revision="usagi-caf-sm6225" />
  <project path="hardware/qcom-caf/sm6225/audio/primary-hal" name="hardware_qcom_audio-ar" remote="fortune" revision="usagi-caf-sm6225" />
  <project path="hardware/qcom-caf/sm6225/data-ipa-cfg-mgr" name="vendor_qcom_opensource_data-ipa-cfg-mgr" remote="fortune" revision="usagi-caf-sm6225" />
  <project path="hardware/qcom-caf/sm6225/dataipa" name="vendor_qcom_opensource_dataipa" remote="fortune" revision="usagi-caf-sm6225" />
  <project path="hardware/qcom-caf/sm6225/display" name="hardware_qcom_display" remote="fortune" revision="usagi-caf-sm6225" />
  <project path="hardware/qcom-caf/sm6225/media" name="hardware_qcom_media" remote="fortune" revision="usagi-caf-sm6225" />

  <!-- Packages -->
  <project path="packages/apps/FortuneCraft" name="packages_apps_FortuneCraft" remote="fortune" />
  <project path="packages/apps/Launcher3" name="packages_apps_Launcher3" remote="fortune" />
  <project path="packages/apps/Settings" name="packages_apps_Settings" remote="fortune" />
  <project path="packages/apps/ThemePicker" name="packages_apps_ThemePicker" remote="fortune" />
  <project path="packages/apps/WallpaperPicker2" name="packages_apps_WallpaperPicker2" remote="fortune" />
  <project path="packages/modules/Connectivity" name="packages_modules_Connectivity" remote="fortune" />

  <!-- System -->
  <project path="system/core" name="system_core" remote="fortune" />
  <project path="system/sepolicy" name="system_sepolicy" remote="fortune" />
  <project path="system/update_engine" name="system_update_engine" remote="fortune" />

  <!-- Vendor -->
  <project path="vendor/addons" name="vendor_addons" remote="fortune" />
  <project path="vendor/fortune" name="vendor_fortune" remote="fortune" />
  <project path="vendor/fortune-preference" name="vendor_fortune-preference" remote="fortune" />
  <project path="vendor/gms" name="android_vendor_gms" remote="fortune-gitea" />

</manifest>
